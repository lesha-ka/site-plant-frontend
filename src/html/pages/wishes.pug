main(class=`main main-${page}`)
    .container.container-narrow 
        +breadcrumb(['Главная'], 'Мои желания')
        h2.wishes-title Мои желания
        ul.wishes-cards
            li.wishes-card 
                img(src="img/wishes/wishes1.jpg" alt="Кустарник")
                .card-inner 
                    form.card-form(method="post")
                        input.card-favorite(type="checkbox" id="wishes1" name="offer1")
                        label(for="wishes1")
                    p.card-name Кустарники
                    p.card-text Гортензия метельчатая в ассортименте (саженцы)
                    span.card-price 22&nbsp;600&nbsp;₽ <del>50&nbsp;990&nbsp;₽</del>
                    form.card-basket
                        input.card-minus(type="button")
                        input.card-count(type="text" value="1")
                        input.card-plus(type="button")
                        button.card-add(type="button")
